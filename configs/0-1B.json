{
  "model": {
    "d_model": 768,
    "d_ff": 1024,
    "n_heads": 12,
    "n_layers": 12,
    "vocab_size": 50257,
    "seq_len": 1024
  },
  "data": {
    "name": "roneneldan/TinyStories",
    "text_column": "text",
    "split": "train",
    "tokenizer": "gpt2"
  },
  "train": {
    "batch_size": 32,
    "n_epochs": 3,
    "optimizer": {
      "type": "adamw",
      "params": {
        "learning_rate": 0.0002
      }
    },
    "clip_grad_norm": 1.0,
    "output_dir": "ae-0-1B",
    "save_checkpoint_steps": 1000,
    "generate": {
      "steps": 100,
      "max_length": 10,
      "prompts": [
        "Neural network is",
        "I am",
        "The meaning of life is",
        "Once upon a time"
      ]
    }
  }
}